unsigned char data[15][15 ]= { {1,2},{2,4},{3,4},{4,6},{5,8},
 {1,2},{2,3},{3,5},{4,7},{5,6}};
unsigned int MAC_result = 0;
unsigned int multiply_accum()
{
int i=0;
unsigned int mac_result=0;
for(i=0;i<10;i++)
{
 mac_result += (data[i][0] * data[i][1]);
}
return (mac_result);
}
int main(void)
{
 MAC_result = multiply_accum();
}
